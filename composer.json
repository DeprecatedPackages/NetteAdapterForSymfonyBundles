{
	"name": "symplify/nette-adapter-for-symfony-bundles",
	"description": "Register Symfony Bundles as Nette Extensions with ease.",
	"keywords": ["nette", "symfony", "dependency injection", "interoperability"],
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "email": "tomas.vot@gmail.com", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": "^7.0",
		"symfony/dependency-injection": "~3.0",
		"symfony/yaml": "~3.0",
		"nette/di": "~2.3",
		"nette/http": "~2.3"
	},
	"require-dev": {
		"symfony/symfony": "~3.0",
		"doctrine/doctrine-bundle": "~1.6",
		"doctrine/orm": "~2.5",
		"hautelook/alice-bundle": "~1.3",
		"league/tactician-bundle": "~0.4",
		"nette/application": "~2.3",
		"nette/bootstrap": "~2.3",
		"tracy/tracy": "~2.3",
		"doctrine/doctrine-fixtures-bundle": "^2.3",
		"symplify/coding-standard": "^1.0.3",
		"phpunit/phpunit": "^5.5"
	},
	"autoload": {
		"psr-4": {
			"Symplify\\NetteAdapaterForSymfonyBundles\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Symplify\\NetteAdapaterForSymfonyBundles\\Tests\\": "tests"
		}
	},
	"scripts": {
		"check-cs": "vendor/bin/symplify-cs check src tests",
		"fix-cs": "vendor/bin/symplify-cs fix src tests"
	}
}
