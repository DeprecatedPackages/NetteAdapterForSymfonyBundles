{
	"name": "symnedi/symfony-bundles-extension",
	"description": "Register Symfony Bundles as Nette Extensions with ease.",
	"keywords": ["nette", "symfony", "dependency injection", "interoperability"],
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "email": "tomas.vot@gmail.com", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": "^5.6|^7.0",
		"symfony/dependency-injection": "~2.7|~3.0",
		"symfony/yaml": "~2.7|~3.0",
		"nette/di": "~2.3",
		"nette/http": "~2.3"
	},
	"require-dev": {
		"doctrine/doctrine-bundle": "~1.5",
		"doctrine/orm": "~2.5",
		"hautelook/alice-bundle": "0.2.*",
		"league/tactician-bundle": "0.2.*",
		"nette/application": "~2.3",
		"nette/bootstrap": "~2.3",
		"nette/robot-loader": "~2.3",
		"tracy/tracy": "~2.3",
		"ongr/elasticsearch-bundle": "0.9.*",
		"zenify/coding-standard": "^3.3",
		"phpunit/phpunit": "^5.5"
	},
	"autoload": {
		"psr-4": {
			"Symnedi\\SymfonyBundlesExtension\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Symnedi\\SymfonyBundlesExtension\\Tests\\": "tests"
		}
	},
	"scripts": {
		"check-cs": "vendor/bin/phpcs src tests --extensions=php --ignore=bootstrap --standard=vendor/zenify/coding-standard/src/ZenifyCodingStandard/ruleset.xml"
	}
}
