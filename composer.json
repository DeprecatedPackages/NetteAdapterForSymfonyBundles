{
	"name": "symnedi/symfony-bundles-extension",
	"description": "Register Symfony Bundles as Nette Extensions with ease.",
	"keywords": ["nette", "symfony", "dependency injection", "interoperability"],
	"license": "MIT",
	"authors": [
		{ "name": "Tomáš Votruba", "email": "tomas.vot@gmail.com", "homepage": "http://tomasvotruba.cz" }
	],
	"require": {
		"php": ">=5.5",
		"symfony/dependency-injection": "^2.3",
		"nette/di": "^2.2"
	},
	"require-dev": {
		"doctrine/doctrine-bundle": "^1.4",
		"doctrine/orm": "~2.5",
		"hautelook/alice-bundle": "0.2.*",
		"league/tactician-bundle": "0.2.*",
		"mockery/mockery": "0.9.*",
		"nette/bootstrap": "~2.3",
		"ongr/elasticsearch-bundle": "0.9.*"
	},
	"autoload": {
		"psr-4": {
			"Symnedi\\SymfonyBundlesExtension\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Symnedi\\SymfonyBundlesExtension\\Tests\\": "tests"
		}
	}
}
