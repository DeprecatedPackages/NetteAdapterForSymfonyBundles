application:
	scanDirs: false
	scanComposer: false

services:
	- Symplify\SymfonyBundlesExtension\Tests\ContainerSource\ParameterStorage(1, [2, 3])
	- Symplify\SymfonyBundlesExtension\Tests\ContainerSource\AutowiredService
	- Symplify\SymfonyBundlesExtension\Tests\ContainerSource\SomeService

extensions:
	symfonyBundles: Symplify\SymfonyBundlesExtension\DI\SymfonyBundlesExtension

symfonyBundles:
	bundles:
		- Doctrine\Bundle\FixturesBundle\DoctrineFixturesBundle
		- League\Tactician\Bundle\TacticianBundle
		alice: Hautelook\AliceBundle\HautelookAliceBundle

	parameters:
		alice:
			locale: cs_CZ
