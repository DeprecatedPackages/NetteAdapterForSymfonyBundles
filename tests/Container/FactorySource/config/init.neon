application:
	scanDirs: false
	scanComposer: false

extensions:
	symfonyBundles: Symnedi\SymfonyBundlesExtension\DI\SymfonyBundlesExtension

symfonyBundles:
	bundles:
		doctrine_bundle: Doctrine\Bundle\DoctrineBundle\DoctrineBundle
	parameters:
		doctrine_bundle:
			dbal:
				default_connection: default
			orm:
				proxy_namespace: Proxies
