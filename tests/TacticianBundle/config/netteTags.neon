application:
	scanDirs: false
	scanComposer: false

extensions:
	symfonyBundles: Symplify\NetteAdapaterForSymfonyBundles\DI\NetteAdapaterForSymfonyBundlesExtension

services:
	-
		class: Symplify\NetteAdapaterForSymfonyBundles\Tests\TacticianBundle\NetteTagsSource\SomeCommandHandler
